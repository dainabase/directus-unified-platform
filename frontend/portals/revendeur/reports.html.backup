            // V√©rifier l'authentification
            if (!PortalApp.checkAuth()) {
                console.warn('‚ùå Utilisateur non authentifi√©');
                window.location.href = '../login.html';
                return;
            }
            
            // V√©rifier que l'utilisateur a le bon r√¥le
            if (PortalApp.currentRole !== 'revendeur') {
                console.warn('‚ùå Acc√®s non autoris√© pour ce r√¥le');
                PortalApp.redirectToDashboard();
                return;
            }
            
            // Initialiser le layout
            if (typeof PortalApp.initLayout === 'function') {
                PortalApp.initLayout();
            }
            
            // Marquer le menu comme actif
            PortalApp.setActiveMenuItem('reports');
            
            // Initialiser le g√©n√©rateur de rapports
            if (typeof ReportGenerator !== 'undefined') {
                window.reportGen = new ReportGenerator();
            }
        });
        
        // G√©n√©rer rapport pr√©d√©fini
        async function generateReport(type) {
            PortalApp.showToast('G√©n√©ration du rapport en cours...', 'info');
            
            // Simuler g√©n√©ration
            setTimeout(() => {
                PortalApp.showToast('Rapport g√©n√©r√© avec succ√®s!', 'success');
                // T√©l√©charger automatiquement
                const link = document.createElement('a');
                link.href = '#';
                link.download = `rapport_${type}_${new Date().toISOString().split('T')[0]}.pdf`;
                link.click();
            }, 2000);
        }
        
        // G√©n√©rer rapport personnalis√©
        function generateCustomReport() {
            console.log('G√©n√©ration rapport personnalis√©...');
            if (window.reportGen) {
                const params = {
                    period: '2024-Q1',
                    format: 'pdf',
                    metrics: ['revenue', 'sales', 'pipeline']
                };
                window.reportGen.generateReport('custom', params);
            }
            bootstrap.Modal.getInstance(document.getElementById('modal-custom-report')).hide();
        }
        
        // Sauvegarder mod√®le de rapport
        function saveReportTemplate() {
            console.log('Sauvegarde du mod√®le...');
            PortalApp.showToast('Mod√®le sauvegard√©', 'success');
        }
        
        // Voir rapports programm√©s
        function viewScheduledReports() {
            console.log('Affichage rapports programm√©s');
            // Faire d√©filer jusqu'√† la section
            document.querySelector('.report-schedule').scrollIntoView({ behavior: 'smooth' });
        }
    </script>
</body>
</html>
