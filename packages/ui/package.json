{
  "name": "@dainabase/ui",
  "version": "0.2.0",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dainabase/directus-unified-platform",
    "directory": "packages/ui"
  },
  "files": [
    "dist",
    "tokens.ts",
    "tailwind.config.ts",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./tokens": "./tokens.ts",
    "./tailwind.config": "./tailwind.config.ts"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "build:vite": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md,mdx}\"",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm run lint && pnpm run typecheck && pnpm run format:check",
    "sb": "storybook dev -p 6006",
    "build:sb:static": "storybook build -o storybook-static",
    "build-storybook": "storybook build -o storybook-static",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:stories": "storybook test --ci",
    "chromatic": "chromatic --exit-zero-on-changes",
    "prepublishOnly": "pnpm run validate && tsc -p tsconfig.json",
    "analyze:bundle": "vite build --mode analyze",
    "perf:profile": "npm run build:vite && ls -lah dist/*.js | head -10",
    "perf:measure": "npm run build:vite && gzip -c dist/index.es.js | wc -c"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-progress": "^1.0.3",
    "@tanstack/react-table": "^8.10.0",
    "@tanstack/react-virtual": "^3.0.0",
    "class-variance-authority": "^0.7.0",
    "cmdk": "^0.2.0",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.309.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.9.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.10.3",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.0",
    "@playwright/test": "^1.40.0",
    "@storybook/addon-a11y": "^7.6.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/blocks": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "autoprefixer": "^10.4.19",
    "chromatic": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.38",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "^7.6.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
