{
  "name": "@dainabase/ui",
  "version": "1.2.0",
  "description": "Dainabase UI Design System - A modern, accessible, and performant component library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "build:optimized": "tsup --config tsup.config.optimized.ts",
    "build:vite": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:size": "npm run build && node -e \"const fs=require('fs');const path=require('path');const dir='./dist';if(fs.existsSync(dir)){const files=fs.readdirSync(dir);let total=0;files.forEach(f=>{if(f.endsWith('.js')||f.endsWith('.mjs')){const s=fs.statSync(path.join(dir,f)).size;total+=s;console.log(f+': '+(s/1024).toFixed(2)+'KB')}});console.log('Total: '+(total/1024).toFixed(2)+'KB')}\"",
    "build:clean": "rm -rf dist && npm run build",
    "build:watch": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:vitest": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:mutation": "stryker run",
    "test:mutation:component": "stryker run --mutate",
    "test:verify": "node scripts/verify-tests.js",
    "test:generate": "node scripts/generate-tests.js",
    "test:gaps": "node scripts/coverage-gap-analyzer.js",
    "test:missing": "node scripts/find-missing-coverage.js",
    "analyze": "node ../../scripts/analyze-bundle.js",
    "analyze:bundle": "npm run build && npm run analyze",
    "analyze:dependencies": "npm ls --depth=0 --prod",
    "analyze:size": "npm run build:size && npm run analyze",
    "analyze:coverage": "node scripts/test-coverage-analyzer.js",
    "analyze:missing": "node scripts/find-missing-coverage.js",
    "monitor:bundle": "npm run build && node scripts/bundle-optimizer.js",
    "monitor:performance": "node scripts/performance-monitor.js",
    "monitor:npm": "node scripts/npm-monitor.js",
    "monitor:all": "npm run monitor:npm && npm run monitor:bundle && npm run monitor:performance",
    "check:size": "npm run build && npm run analyze:bundle",
    "check:pages": "node scripts/check-github-pages.js",
    "update:metrics": "node scripts/update-metrics.js",
    "coverage": "jest --coverage",
    "coverage:gaps": "npm run test:coverage && npm run analyze:coverage",
    "coverage:missing": "npm run test:missing",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "sb": "storybook dev -p 6006",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "npm run build-storybook && echo 'Run GitHub Actions to deploy'",
    "chromatic": "chromatic --project-token=${CHROMATIC_PROJECT_TOKEN}",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist coverage .stryker-tmp reports playwright-report test-results storybook-static metrics",
    "ci:test": "npm run test:ci && npm run test:e2e && npm run check:size",
    "ci:monitor": "npm run monitor:all",
    "report:bundle": "npm run analyze > bundle-report.txt",
    "report:performance": "node scripts/generate-performance-report.js",
    "report:npm": "npm run monitor:npm && cat metrics/npm-report.md",
    "report:coverage": "npm run coverage:gaps && npm run coverage:missing",
    "report:all": "npm run report:npm && npm run report:bundle && npm run report:performance && npm run report:coverage",
    "optimize:check": "npm run analyze:size && npm run monitor:performance",
    "optimize:bundle": "npm run build:optimized",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run build && npm run test:ci",
    "release:alpha": "npm version prerelease --preid=alpha && npm publish --tag alpha",
    "release:beta": "npm version prerelease --preid=beta && npm publish --tag beta",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-primitive": "^1.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7"
  },
  "optionalDependencies": {
    "@tanstack/react-table": "^8.10.7",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "embla-carousel-react": "^8.0.0-rc14",
    "i18next": "^23.7.16",
    "lucide-react": "^0.294.0",
    "react-day-picker": "^8.9.1",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^14.0.0",
    "recharts": "^2.10.3",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.25",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-vite": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@stryker-mutator/core": "^8.0.0",
    "@stryker-mutator/vitest-runner": "^8.0.0",
    "@stryker-mutator/typescript-checker": "^8.0.0",
    "@stryker-mutator/html-reporter": "^8.0.0",
    "@stryker-mutator/json-reporter": "^8.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.3.0",
    "chromatic": "^10.2.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "^23.0.1",
    "playwright": "^1.40.1",
    "postcss": "^8.4.32",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "^8.1.11",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.0.4",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "keywords": [
    "ui",
    "design-system",
    "components",
    "react",
    "typescript",
    "tailwind",
    "radix-ui",
    "accessible",
    "i18n",
    "enterprise",
    "lazy-loading",
    "optimized",
    "performance",
    "virtualized",
    "dainabase"
  ],
  "author": "Dainabase",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dainabase/directus-unified-platform.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/dainabase/directus-unified-platform/issues"
  },
  "homepage": "https://github.com/dainabase/directus-unified-platform/tree/main/packages/ui#readme",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./lazy/*": {
      "import": "./dist/lazy/*.mjs",
      "require": "./dist/lazy/*.js"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
