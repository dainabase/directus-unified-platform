{
  "name": "@dainabase/ui",
  "version": "1.0.0-alpha.1",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dainabase/directus-unified-platform",
    "directory": "packages/ui"
  },
  "files": [
    "dist",
    "tokens.ts",
    "tailwind.config.ts",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./tokens": "./tokens.ts",
    "./tailwind.config": "./tailwind.config.ts",
    "./lazy": {
      "types": "./dist/components-lazy.d.ts",
      "import": "./dist/components-lazy.js",
      "require": "./dist/components-lazy.cjs"
    },
    "./core": {
      "types": "./dist/chunks/core.d.ts",
      "import": "./dist/chunks/core.js"
    },
    "./charts": {
      "types": "./dist/chunks/charts.d.ts",
      "import": "./dist/chunks/charts.js"
    },
    "./data-grid": {
      "types": "./dist/chunks/data-grid.d.ts",
      "import": "./dist/chunks/data-grid.js"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "pnpm build:clean && pnpm build:lib && pnpm build:types && pnpm build:size",
    "build:clean": "rm -rf dist",
    "build:lib": "vite build --mode production",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist",
    "build:analyze": "vite build --mode analyze",
    "build:size": "size-limit",
    "build:optimize": "pnpm build:clean && vite build --mode production && pnpm build:size",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "sb": "storybook dev -p 6006",
    "build:sb": "storybook build -o storybook-static",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:stories": "storybook test --ci",
    "chromatic": "chromatic --exit-zero-on-changes",
    "prepublishOnly": "pnpm build:optimize",
    "size": "size-limit",
    "perf": "lighthouse --output html --output-path ./perf-report.html"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-primitive": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "lucide-react": "^0.309.0",
    "tailwind-merge": "^2.2.0"
  },
