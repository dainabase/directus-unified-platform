  "size-limit": [
    {
      "name": "Core bundle",
      "path": "dist/index.js",
      "limit": "45 KB",
      "gzip": true
    },
    {
      "name": "Core + React",
      "path": "dist/index.js",
      "import": "{ Button, Card, Badge }",
      "limit": "48 KB"
    },
    {
      "name": "Lazy components",
      "path": "dist/components-lazy.js",
      "limit": "5 KB"
    },
    {
      "name": "Core utilities",
      "path": "dist/chunks/utils.js",
      "limit": "3 KB"
    }
  ],
